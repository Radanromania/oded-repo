<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>NRTRDE.html</welcome-file>
  </welcome-file-list>

 <servlet>
    <servlet-name>nrtrdeServlet</servlet-name>
    <servlet-class>com.mts.nrtrde.server.NRTRDEServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>nrtrdeServlet</servlet-name>
    <url-pattern>/nrtrde/NRTRDEService</url-pattern>
  </servlet-mapping>
</web-app>
